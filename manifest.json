{
  "manifest_version": 2,
  "name": "Github Material Plugins - Muhammed Zaim",
  "short_name": "G.M.P - M.Z",
  "version": "1.1",
  "description": "Github'ı material tasarıma geçirmenin tam zamanı.",

  "permissions": [
    "*://github.com/*",
    "*://www.github.com/*"
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Github Material Plugins - Muhammed Zaim"
  },

  "content_scripts": [{
    "matches": [
      "*://github.com/*",
    "*://www.github.com/*"
  ],
    "css": [
      "/style1.css"
    ]
  }]
}
